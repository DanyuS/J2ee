<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="entity.Manager" table="manager">
		<meta attribute="class-description">
			This class contains the member detail.
		</meta>
		<id name="maid" type="java.lang.Integer" column="maid">
			<generator class="native"/>
		</id>
		<property name="managerName" column="managerName" type="java.lang.String"/>
		<property name="managerPassword" column="managerPassword" type="java.lang.String"/>
		<property name="identity" column="identity" type="java.lang.String"/>
		<property name="account" column="account" type="java.lang.Double"/>
	</class>
</hibernate-mapping>