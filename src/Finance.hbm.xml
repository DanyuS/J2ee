<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="entity.Finance" table="finance">
		<meta attribute="class-description">
			This class contains the finance detail.
		</meta>
		<id name="financeId" type="java.lang.Integer" column="financeId">
			<generator class="native"/>
		</id>
		<property name="rid" column="rid" type="java.lang.String"/>
		<property name="mid" column="mid" type="java.lang.Integer"/>
		<property name="date" column="date" type="java.lang.String"/>
		<property name="account" column="account" type="java.lang.Double"/>
		<property name="state" column="state" type="java.lang.Integer"/>
	</class>
</hibernate-mapping>