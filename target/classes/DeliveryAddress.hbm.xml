<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="entity.DeliveryAddress" table="deliveryaddress">
		<meta attribute="class-description">
			This class contains the address detail.
		</meta>
		<id name="id" type="java.lang.Integer" column="id">
			<generator class="native"/>
		</id>
		<property name="addrId" column="addrId" type="java.lang.String"/>
		<property name="name" column="name" type="java.lang.String"/>
		<property name="phone" column="phone" type="java.lang.String"/>
		<property name="province" column="province" type="java.lang.String"/>
		<property name="city" column="city" type="java.lang.String"/>
		<property name="district" column="district" type="java.lang.String"/>
		<property name="street" column="street" type="java.lang.String"/>
	</class>
</hibernate-mapping>