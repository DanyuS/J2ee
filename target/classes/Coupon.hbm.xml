<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="entity.Coupon" table="coupon">
		<meta attribute="class-description">
			This class contains the member detail.
		</meta>
		<id name="cid" type="java.lang.Integer" column="cid">
			<generator class="native"/>
		</id>
		<property name="couponId" column="couponId" type="java.lang.String"/>
		<property name="couponName" column="couponName" type="java.lang.String"/>
		<property name="minPayment" column="minPayment" type="java.lang.Double"/>
		<property name="discount" column="discount" type="java.lang.Double"/>
		<property name="restaurantRange" column="restaurantRange" type="java.lang.String"/>
		<property name="startTime" column="startTime" type="java.lang.String"/>
		<property name="endTime" column="endTime" type="java.lang.String"/>
		<property name="level" column="level" type="java.lang.Integer"/>
	</class>
</hibernate-mapping>